
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@section featured{
<div class="page-title">
    <h2><span class="fa fa-arrow-circle-o-left"></span> REPORTES GENERALES </h2>
</div>
<div class="page-content-wrap">
    <div class="row">
        <div class="panel panel-default">
