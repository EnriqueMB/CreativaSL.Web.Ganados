                                        <div class="input-group date" id="FechaFin" >
                                            <input type="text" class="form-control daterange" disabled="disabled" id="fec2"/>
                                            <span class="input-group-addon add-on"><span class="glyphicon glyphicon-calendar"></span></span>
                                        </div>
                                        <div id="result2"></div>
                                    </div>
                                </div>
                            </div>
