                                        <div class="form-group">
                                            <div class="col-md-12 controlError">
                                                <label class="col-md-2 control-label">Destinatario</label>
                                                <div class="col-md-10 col-xs-12">
                                                    <div class="input-group">
                                                        <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                                        @Html.DropDownListFor(model => model.Trayecto.Destinatario.IDCliente, new SelectList(Model.Trayecto.ListaDestinatario, "IDCliente", "NombreRazonSocial"), new { @class = "form-control select ", data_live_search = "true", data_style = "form-control" })
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-12 controlError">
                                                <label class="col-md-2 control-label">Domicilio</label>
                                                <div class="col-md-10 col-xs-12">
                                                    <div class="input-group">
                                                        <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                                        @Html.TextBoxFor(model => model.Trayecto.LugarDestino.Direccion, new { id = "Trayecto_LugarDestino_Direccion", @class = "form-control", @readonly = "readonly" })
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-12 controlError">
                                                <label class="col-md-2 control-label">Se entregará en:</label>
                                                <div class="col-md-10 col-xs-12">
                                                    <div class="input-group">
                                                        <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                                        @Html.TextBoxFor(model => model.Trayecto.LugarDestino.descripcion, new { id = "Trayecto_LugarDestino_descripcion", @class = "form-control", placeholder = "Lugar de entrega", @readonly = "readonly" })
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                            <div class="form-group">
                                <div class="col-md-12">
                                    <div id="floating-panel">
                                        <b>Origen: </b>
                                        <select id="Trayecto_LugarOrigen_id_lugar" name="Trayecto.LugarOrigen.id_lugar">
                                            @foreach (var lugar in Model.Trayecto.ListaLugarOrigen)
                                        {
                                            <option value="@lugar.id_lugar" data-latitud="@lugar.latitud" data-longitud="@lugar.longitud" data-direccion="@lugar.Direccion"
                                                    @if (Model.Trayecto.LugarOrigen.id_lugar == lugar.id_lugar) { <text>
                                                    selected="selected"
                                                    </text> }>
                                                @lugar.descripcion
                                            </option>
                                    }
                                        </select>

                                        <b>Destino: </b>

                                        <select id="Trayecto_LugarDestino_id_lugar" name="Trayecto.LugarDestino.id_lugar">
                                            @foreach (var lugar in Model.Trayecto.ListaLugarDestino)
                                        {
                                            <option value="@lugar.id_lugar" data-latitud="@lugar.latitud" data-longitud="@lugar.longitud" data-direccion="@lugar.Direccion"
                                                    @if (Model.Trayecto.LugarDestino.id_lugar == lugar.id_lugar) { <text>
                                                    selected="selected"
                                                    </text> }>
                                                @lugar.descripcion
                                            </option>
                                    }
                                        </select>
                                    </div>
                                    <div id="map"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="panel-footer">
                        <input type="submit" value="Guardar" class="btn btn-primary pull-right">
                        <a href="@Url.Action("Index", "Flete")" class="btn btn-danger"><i class="icon-prev"></i>Regresar</a>
                    </div>
                </div>
        }
                    </div>
                </div>
