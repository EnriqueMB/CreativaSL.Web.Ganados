@model CreativaSL.Web.Ganados.Models.FleteModels

@{
    ViewBag.Title = "AC_Flete";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@section css
{
    <!--FILEINPUT-->
    <link href="@Url.Content("~/Content/js/plugins/uploadFiles/fileinput.css")" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />

    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/select/1.2.5/css/select.bootstrap.min.css">

    <link href="@Url.Content("~/Content/js/plugins/uploadFiles/theme.css")" rel="stylesheet" />
    <style>
        #map {
            height: 400px;
            width: 100%;
        }
        /* Optional: Makes the sample page fill the window. */
        html, body {
            height: 100%;
            margin: 0;
            padding: 0;
        }

        #floating-panel {
            display: block;
            top: 10px;
            left: 25%;
            z-index: 5;
            background-color: #fff;
            padding: 5px;
            border: 1px solid #999;
            text-align: center;
            font-family: 'Roboto','sans-serif';
            line-height: 30px;
            padding-left: 10px;
        }

        .form-control[disabled], .form-control[readonly] {
            color: #000 !important;
        }

       .btnCSL {
            border-color: #999;
            color: #1b1e24;
            border-radius: 20%;
       }
        .errorCSLGanado{
            border-color: #E04B4A !important;
       }
        .okCSLGanado{
            border-color: #95b75d !important;
       }

    </style>
}
@section featured
{
    @{
        var message = TempData["message"] ?? string.Empty;
        var typemessage = TempData["typemessage"] ?? string.Empty;
    }
    <div class="col-md-12">
        <h3>
            @Html.Label("Folio del flete: " + Model.Folio, new { id = "Folio", style="float: right" })
        </h3>
    </div>

    <div class="row">
        <div id="Error" class="progress-bar progress-bar-striped active errorCSL" style="display:none; width:100%">
            <h3></h3>
        </div>
        <div id="Success" class="progress-bar progress-bar-striped active successCSL" style="display:none; width:100%">
            <h3></h3>
        </div>
        <!--INICIA TABS-->
        <div class="panel panel-default tabs">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs" role="tablist">
                <li id="liCliente" role="presentation" class="active">  <a id="tabCliente" class="pestaña cslTablist" href="#cliente" aria-controls="cliente" role="tab" data-toggle="tab">Cliente</a></li>
                <li id="liTrayecto" role="presentation" class="disabled"><a id="tabTrayecto" class="pestaña cslTablist" href="#trayecto" aria-controls="trayecto" role="tab" data-toggle="">Trayecto</a></li>
                <li id="liDocumentacion" role="presentation" class="disabled"><a id="tabDocumentacion" class="pestaña cslTablist" href="#documentacion" aria-controls="documentacion" role="tab" data-toggle="">Documentación</a></li>
                <li id="liProducto" role="presentation" class="disabled"><a id="tabProducto" class="pestaña cslTablist" href="#producto" aria-controls="producto" role="tab" data-toggle="">Productos</a></li>
                <li id="liCobro" role="presentation" class="disabled"><a id="tabCobro" class="pestaña cslTablist" href="#cobro" aria-controls="cobro" role="tab" data-toggle="">Cobro</a></li>
            </ul>

            <!-- Tab panes -->
            <div class="tab-content">
                <div role="tabpanel" class="tab-pane active" id="cliente">
                    <div class="col-md-12">
                        @{ Html.EnableClientValidation(false); }
                        @using (Html.BeginForm("AC_Cliente", "Flete", FormMethod.Post, new { id = "Frm_AC_Cliente", @class = "form-horizontal", autocomplete = "off" }))
                    {
                        @Html.HiddenFor(model => model.id_flete);
                    @Html.AntiForgeryToken()

                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title">Información del <strong>Cliente</strong></h3>
                            <ul class="panel-controls"></ul>
                        </div>
                        <div class="panel-body">
                            @Html.ValidationSummary(false, "Cheque los siguientes problemas:")
                            <div id="validation_summary_AC_Cliente" class="field_error "></div><br />
                            <div class="form-group">
                                <div class="col-md-12 controlError">
                                    <label class="col-md-1 control-label">Linea fletera</label>
                                    <div class="col-md-11 col-xs-12">
                                        <div class="input-group">
                                            <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                            @Html.DropDownListFor(model => model.Empresa.IDEmpresa, new SelectList(Model.ListaEmpresa, "IDEmpresa", "RazonFiscal"), new { id = "Empresa_IDEmpresa", @class = "form-control select", data_live_search = "true", data_style = "form-control" })
                                        </div>
                                        @Html.ValidationMessageFor(model => model.Empresa.IDEmpresa)
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-6 controlError">
                                    <label class="col-md-2 control-label">Nombre del cliente</label>
                                    <div class="col-md-10 col-xs-12">
                                        <div class="input-group">
                                            <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                            <select id="Cliente_IDCliente" name="Cliente.IDCliente" class="form-control select" data-live-search="true" , data-style="form-control">
                                                @foreach (var cliente in Model.ListaCliente)
                                            {
                                                <option value="@cliente.IDCliente" data-rfc="@cliente.RFC"
                                                        @if (Model.Cliente.IDCliente == cliente.IDCliente) { <text>
                                                        selected="selected"
                                                        </text> }>
                                                    @cliente.NombreRazonSocial
                                                </option>
                                        }
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 controlError">
                                    <label class="col-md-2 control-label">R.F.C. del cliente</label>
                                    <div class="col-md-10 col-xs-12">
                                        <div class="input-group">
                                            <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                            @Html.TextBoxFor(model => model.Cliente.RFC, new { id = "Cliente_RFC", @class = "form-control", placeholder = "R.F.C del Cliente.", @readonly = "readonly" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-6 controlError">
                                    <label class="col-md-2 control-label">Chofer</label>
                                    <div class="col-md-10 col-xs-12">
                                        <div class="input-group">
                                            <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                            @Html.DropDownListFor(model => model.Chofer.IDChofer, new SelectList(Model.ListaChofer, "IDChofer", "Nombre"), new { @class = "form-control select", data_live_search = "true", data_style = "form-control" })
                                        </div>
                                        @Html.ValidationMessageFor(model => model.Chofer.IDChofer)
                                    </div>
                                </div>

                                <div class="col-md-6 controlError">
                                    <label class="col-md-2 control-label">Vehículo</label>
                                    <div class="col-md-10 col-xs-12">
                                        <div class="input-group">
                                            <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                            @Html.DropDownListFor(model => model.Vehiculo.IDVehiculo, new SelectList(Model.ListaVehiculo, "IDVehiculo", "nombreMarca", "nombreTipoVehiculo", 1), new { @class = "form-control select", data_live_search = "true", data_style = "form-control" })
                                        </div>
                                        @Html.ValidationMessageFor(model => model.Vehiculo.IDVehiculo)
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-6 controlError">
                                    <label class="col-md-2 control-label">Jaula</label>
                                    <div class="col-md-10 col-xs-12">
                                        <div class="input-group">
                                            <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                            @Html.DropDownListFor(model => model.Jaula.IDJaula, new SelectList(Model.ListaJaula, "IDJaula", "Matricula"), new { @class = "form-control select", data_live_search = "true", data_style = "form-control" })
                                        </div>
                                        @Html.ValidationMessageFor(model => model.Jaula.IDJaula)
                                    </div>
                                </div>
                                <div class="col-md-6 controlError">
                                    <label class="col-md-2 control-label">Remolque</label>
                                    <div class="col-md-10 col-xs-12">
                                        <div class="input-group">
                                            <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                            @Html.DropDownListFor(model => model.Remolque.IDRemolque, new SelectList(Model.ListaRemolque, "IDRemolque", "placa"), new { @class = "form-control select", data_live_search = "true", data_style = "form-control" })
                                        </div>
                                        @Html.ValidationMessageFor(model => model.Remolque.IDRemolque)
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-6 controlError">
                                    <label class="col-md-2 control-label">Kilómetraje Inicial</label>
                                    <div class="col-md-10 col-xs-12">
                                        <div class="input-group">
                                            <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                            @Html.TextBoxFor(model => model.kmInicialVehiculo, new { id = "KmInicial", @class = "form-control", placeholder = "Introduzca el km. inicial del vehículo." })
                                        </div>
                                        @Html.ValidationMessageFor(model => model.kmInicialVehiculo)
                                    </div>
                                </div>
                                <div class="col-md-6 controlError">
                                    <label class="col-md-2 control-label">Fecha tentativa de entrega</label>
                                    <div class="col-md-10 col-xs-12">
                                        <div class="input-group">
                                            <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                            @Html.TextBoxFor(model => model.FechaTentativaEntrega, "{0:dd/MM/yyyy}", new { id = "FechaTentativaEntrega", autocomplete = "off", @class = "form-control datepicker", @readonly = "readonly" })
                                        </div>
                                        @Html.ValidationMessageFor(model => model.FechaTentativaEntrega)
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="panel-footer">
                            <input type="submit" value="Guardar" class="btn btn-primary pull-right">
                            <a href="@Url.Action("Index", "Flete")" class="btn btn-danger"><i class="icon-prev"></i>Regresar</a>
                        </div>
                    </div>
            }
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane" id="trayecto">
                    <div class="col-md-12">
                        @{ Html.EnableClientValidation(false); }
                        @using (Html.BeginForm("AC_Trayecto", "Flete", FormMethod.Post, new { id = "frm_AC_Trayecto", @class = "form-horizontal", autocomplete = "off" }))
                    {
                        @Html.HiddenFor(model => model.id_flete);
                    @Html.HiddenFor(model => model.Trayecto.id_trayecto);
                @Html.AntiForgeryToken()

                <div class="panel panel-default">
