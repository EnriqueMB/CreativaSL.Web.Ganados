@model CreativaSL.Web.Ganados.Models.VentaDetalleModels
@{
    ViewBag.Title = "VentaDetalles";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
@section css
{
    <!--CSS GENERAL-->
    
    <style>
        .vista_previa {
            cursor: pointer;
        }
    </style>
}
@section featured
{
    @{
        var message = TempData["message"] ?? string.Empty;
        var typemessage = TempData["typemessage"] ?? string.Empty; ;
    }
    <div class="row">
        <div id="Error" class="progress-bar progress-bar-striped active" style="display:none; width:100%">
            <h3></h3>
        </div>
        <div id="Success" class="progress-bar progress-bar-striped active" style="display:none; width:100%">
            <h3></h3>
        </div>
        <!--INICIA TABS-->
        <div class="panel panel-default tabs">

            <!-- Nav tabs -->
            <ul class="nav nav-tabs" role="tablist">
                <li role="presentation" class="active"><a href="#general" aria-controls="general" role="tab" data-toggle="tab">General</a></li>
                <li role="presentation" class=""><a href="#ganado" aria-controls="ganado" role="tab" data-toggle="tab">Ganado</a></li>
                <li role="presentation" class=""><a href="#pagos" aria-controls="pagos" role="tab" data-toggle="tab">Pagos</a></li>
                <li role="presentation" class=""><a href="#cobros" aria-controls="cobros" role="tab" data-toggle="tab">Cobros</a></li>
                <li role="presentation" class=""><a href="#documentos" aria-controls="documentos" role="tab" data-toggle="tab">Documentos</a></li>
            </ul>

            <!-- Tab panes -->
            <div class="tab-content">
                <div role="tabpanel" class="tab-pane active" id="general">
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">INFORMACION</h3>
                                @Html.HiddenFor(model => model.Id_venta)
                                <ul class="panel-controls">
                                    <li><a href="#" class="panel-collapse exsa"><span class="fa fa-angle-down"></span></a></li>
                                    <li><a href="#" class="panel-fullscreen exsa"><span class="fa fa-expand"></span></a></li>
                                </ul>
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="form-group">
                                        <div class="col-md-6">
                                            <h4>Datos de la venta</h4>
                                            <dl class="dl-horizontal">
                                                <dt>Folio</dt>
                                                <dd>@Model.VentaFolio</dd>
                                                <dt>Sucursal</dt>
                                                <dd>@Model.VentaSucursal</dd>
                                                <dt>Fecha finalizada</dt>
                                                <dd>@Model.VentaFecha</dd>
                                                <dt>Merma</dt>
                                                <dd>@Model.VentaMerma</dd>
                                                <dt>Observación</dt>
                                                <dd>@Model.VentaObservacion</dd>
                                            </dl>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-md-6">
                                            <h4>Datos del flete</h4>
                                            <dl class="dl-horizontal">
                                                <dt>Linea fletera</dt>
                                                <dd>@Model.FleteLineaFletera</dd>
                                                <dt>Lugar origen</dt>
                                                <dd>@Model.FleteLugarOrigen</dd>
                                                <dt>Lugar destino</dt>
                                                <dd>@Model.FleteLugarDestino</dd>
                                                <dt>Chofer</dt>
                                                <dd>@Model.FleteNombreChofer</dd>
                                                <dt>Precio del flete</dt>
                                                <dd>@Model.FletePrecio.ToString("C2")</dd>
                                            </dl>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group">
                                        <div class="col-md-6">
                                            <h4>Datos del Cliente</h4>
                                            <dl class="dl-horizontal">
                                                <dt>Nombre completo</dt>
                                                <dd>@Model.ClienteNombreRazonSocialCompleto</dd>
                                                <dt>Regimen fiscal</dt>
                                                <dd>@Model.ClienteRegimenFiscal</dd>
                                                <dt>Dirección</dt>
                                                <dd>@Model.ClienteDireccion</dd>
                                                <dt>P.S.G.</dt>
                                                <dd>@Model.ClientePSG</dd>
                                            </dl>
                                        </div>

                                        <div class="col-md-6">
                                            <h4>Datos de la Empresa</h4>
                                            <dl class="dl-horizontal">
                                                <dt>Nombre de la empresa</dt>
                                                <dd>@Model.EmpresaRazonSocial</dd>
                                                <dt>Dirección</dt>
                                                <dd>@Model.EmpresaDireccion</dd>
                                                <dt>R.F.C.</dt>
                                                <dd>@Model.EmpresaRFC</dd>
                                                <dt>P.S.G.</dt>
                                                <dd>@Model.EmpresaPSG</dd>
                                            </dl>
                                        </div>
                                    </div>
                                </div>
                                <hr />
                                

                            </div>
                            <div class="panel-footer">
                                <a href="@Url.Action("Index", "Venta")" class="btn btn-danger"><i class="icon-prev"></i>Regresar</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane" id="ganado">
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">GANADO</h3>
                                <ul class="panel-controls">
                                    <li><a href="#" class="panel-collapse exsa"><span class="fa fa-angle-down"></span></a></li>
                                    <li><a href="#" class="panel-fullscreen exsa"><span class="fa fa-expand"></span></a></li>
                                </ul>
                            </div>
                            <div class="panel panel-body">
                                <div class="table-responsive">
                                    <table id="tbl_generalGanado" class="table">
                                        <thead>
                                            <tr>
                                                <th>Género</th>
                                                <th>Cantidad</th>
                                                <th>Total de kilos - peso</th>
                                                <th>Total de kilos - repeso</th>
                                                <th>Merma promedio (%)</th>
                                                <th>Subtotal ($)</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">GANADO MACHOS</h3>
                                <ul class="panel-controls">
                                    <li><a href="#" class="panel-collapse exsa"><span class="fa fa-angle-down"></span></a></li>
                                    <li><a href="#" class="panel-fullscreen exsa"><span class="fa fa-expand"></span></a></li>
                                </ul>
                            </div>
                            <div class="panel panel-body">
                                <div class="table-responsive">
                                    <table id="tbl_ganadoDetalleMachos" class="table">
                                        <thead>
                                            <tr>
                                                <th>Número de arete</th>
                                                <th>Género</th>
                                                <th>Peso inicial (kg.)</th>
                                                <th>Peso final - Repeso (kg.)</th>
                                                <th>Diferencia de peso(kg.)</th>
                                                <th>Merma obtenida (%)</th>
                                                <th>Peso pagado (kg.)</th>
                                                <th>Precio por kilo ($)</th>
                                                <th>Subtotal ($)</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">GANADO HEMBRAS</h3>
                                <ul class="panel-controls">
                                    <li><a href="#" class="panel-collapse exsa"><span class="fa fa-angle-down"></span></a></li>
                                    <li><a href="#" class="panel-fullscreen exsa"><span class="fa fa-expand"></span></a></li>
                                </ul>
                            </div>
                            <div class="panel panel-body">
                                <div class="table-responsive">
                                    <table id="tbl_ganadoDetalleHembras" class="table">
                                        <thead>
                                            <tr>
                                                <th>Número de arete</th>
                                                <th>Género</th>
                                                <th>Peso inicial (kg.)</th>
                                                <th>Peso final - Repeso (kg.)</th>
                                                <th>Diferencia de peso(kg.)</th>
                                                <th>Merma obtenida (%)</th>
                                                <th>Peso pagado (kg.)</th>
                                                <th>Precio por kilo ($)</th>
                                                <th>Subtotal ($)</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="panel-footer">
                                <a href="@Url.Action("Index", "Venta")" class="btn btn-danger"><i class="icon-prev"></i>Regresar</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane" id="pagos">
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">PRODUCTOS O SERVICIOS POR PAGAR REALIZADOS DURANTE LA COMPRA</h3>
                                <ul class="panel-controls">
                                    <li><a id="p4" class="exsa"><span class="fa fa-angle-down"></span></a></li>
                                    <li><a href="#" class="panel-fullscreen exsa"><span class="fa fa-expand"></span></a></li>
                                </ul>
                            </div>
                            <div class="panel-body">
                                <div class="table-responsive">
                                    <div class="table-responsive">
                                        <table id="tbl_docxpagardetalles" class="table">
                                            <thead>
                                                <tr>
                                                    <th>Descripción</th>
                                                    <th>Cantidad</th>
                                                    <th>Precio unitario</th>
                                                    <th>Subtotal</th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">PAGOS REALIZADOS DURANTE LA COMPRA</h3>
                                <ul class="panel-controls">
                                    <li><a id="p4" class="exsa"><span class="fa fa-angle-down"></span></a></li>
                                    <li><a href="#" class="panel-fullscreen exsa"><span class="fa fa-expand"></span></a></li>
                                </ul>
                            </div>
                            <div class="panel-body">
                                <div class="table-responsive">
                                    <div class="table-responsive">
                                        <table id="tbl_docxpagardetallesPagos" class="table">
                                            <thead>
                                                <tr>
                                                    <th>Tipo de pago</th>
                                                    <th>Banco</th>
                                                    <th>Número: Cuenta / Tarjeta / Clabe</th>
                                                    <th>Observación</th>
                                                    <th>Fecha realizada</th>
                                                    <th>Imagen</th>
                                                    <th>Cantidad</th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <div class="panel-footer">
                                <a href="@Url.Action("Index", "Venta")" class="btn btn-danger"><i class="icon-prev"></i>Regresar</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane" id="cobros">
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">PRODUCTOS O SERVICIOS POR COBRAR REALIZADOS DURANTE LA COMPRA</h3>
                                <ul class="panel-controls">
                                    <li><a id="p4" class="exsa"><span class="fa fa-angle-down"></span></a></li>
                                    <li><a href="#" class="panel-fullscreen exsa"><span class="fa fa-expand"></span></a></li>
                                </ul>
                            </div>
                            <div class="panel-body">
                                <div class="table-responsive">
                                    <div class="table-responsive">
                                        <table id="tbl_docxcobrardetalles" class="table">
                                            <thead>
                                                <tr>
                                                    <th>Producto o Servicio</th>
                                                    <th>Descripción</th>
                                                    <th>Cantidad</th>
                                                    <th>Precio unitario</th>
                                                    <th>Impuestos retenidos</th>
                                                    <th>Impuestos trasladados</th>
                                                    <th>Impuestos</th>
                                                    <th>Subtotal</th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">COBROS REALIZADOS DURANTE LA COMPRA</h3>
                                <ul class="panel-controls">
                                    <li><a id="p4" class="exsa"><span class="fa fa-angle-down"></span></a></li>
                                    <li><a href="#" class="panel-fullscreen exsa"><span class="fa fa-expand"></span></a></li>
                                </ul>
                            </div>
                            <div class="panel-body">
                                <div class="table-responsive">
                                    <div class="table-responsive">
                                        <table id="tbl_docxcobrardetallesPagos" class="table">
                                            <thead>
                                                <tr>
                                                    <th>Tipo de pago</th>
                                                    <th>Banco</th>
                                                    <th>Número: Cuenta / Tarjeta / Clabe</th>
                                                    <th>Observación</th>
                                                    <th>Fecha realizada</th>
                                                    <th>Imagen</th>
                                                    <th>Cantidad</th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <div class="panel-footer">
                                <a href="@Url.Action("Index", "Venta")" class="btn btn-danger"><i class="icon-prev"></i>Regresar</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane" id="documentos">
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">DOCUMENTOS UTILIZADOS DURANTE LA COMPRA</h3>
                                <ul class="panel-controls">
                                    <li><a id="p4" class="exsa"><span class="fa fa-angle-down"></span></a></li>
                                    <li><a href="#" class="panel-fullscreen exsa"><span class="fa fa-expand"></span></a></li>
                                </ul>
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="form-group">
                                        <div class="col-md-6 ">
                                            <label class="col-md-4 control-label">Costo de la documentación:</label>
                                            <div class="col-md-8 col-xs-12">
                                                <div class="input-group">
                                                    <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                                    @Html.TextBoxFor(model => model.DocumentosPrecioDocumentacion, "{0:f2}", new { @class = "form-control", @readonly = "readonly" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6 controlError">
                                            <label class="col-md-2 control-label">Tipo de salida:</label>
                                            <div class="col-md-10 col-xs-12">
                                                <div class="input-group">
                                                    <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
                                                    @Html.TextBoxFor(model => model.DocumentosTipoSalidaDocumentacion, new { @class = "form-control", @readonly = "readonly" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="table-responsive">
                                        <table id="tbl_documentos" class="table">
                                            <thead>
                                                <tr>
                                                    <th>Tipo de documentos</th>
                                                    <th>Clave</th>
                                                    <th>Imagen</th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>
                            <div class="panel-footer">
                                <a href="@Url.Action("Index", "Venta")" class="btn btn-danger"><i class="icon-prev"></i>Regresar</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--TERMINA TABS-->
    </div>
    <!-- Modal -->
    <div class="modal fade" id="modalImagen" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Imagen</h4>
                </div>
                <div class="modal-body">
                    <img id="imagen_previa" src="" alt="Imagen_previa" />
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
                </div>
            </div>

        </div>
    </div>
    <script>
        var message     = '@message';
        var typemessage = '@typemessage';
    </script>
}

